{
    "name": "pehapkari/website",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "symplify/statie": "^1.3"
    },
    "require-dev": {
        "doctrine/annotations": "^1.3",
        "doctrine/cache": "^1.6",
        "latte/latte": "^2.4",
        "nette/application": "^2.4",
        "nette/forms": "^2.4",
        "nette/security": "^2.4",
        "symfony/console": "^3.2",
        "symfony/event-dispatcher": "^3.2",
        "symfony/expression-language": "^3.2",
        "symfony/intl": "^3.2",
        "symfony/validator": "^3.2",

        "phpstan/phpstan": "^0.6",
        "phpunit/phpunit": "^5.7",
        "symplify/coding-standard": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Pehapkari\\Website\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pehapkari\\Website\\Tests\\": "tests"
        }
    },
    "scripts": {
        "check-cs": [
            "vendor/bin/symplify-cs check src tests"
        ],
        "fix-cs": [
            "vendor/bin/symplify-cs fix src tests"
        ],
        "phpstan": [
            "vendor/bin/phpstan analyse --level=5 --configuration=phpstan.neon src tests"
        ]
    }
}
