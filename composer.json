{
    "name": "pehapkari/website",
    "license": "MIT",
    "require": {
        "symplify/statie": "^1.3"
    },
    "require-dev": {
        "nette/application": "^2.4",
        "nette/security": "^2.4",
        "nette/forms": "^2.4",
        "latte/latte": "^2.4",
        "symfony/console": "^3.1",
        "symfony/event-dispatcher": "^3.1",

        "phpunit/phpunit": "^5.7",
        "symplify/coding-standard": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Pehapkari\\Website\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pehapkari\\Website\\Tests\\": "tests"
        }
    },
    "scripts": {
        "check-cs": [
            "vendor/symplify/coding-standard/bin/symplify-cs check src",
            "vendor/symplify/coding-standard/bin/symplify-cs check tests"
        ],
        "fix-cs": [
            "vendor/symplify/coding-standard/bin/symplify-cs fix src",
            "vendor/symplify/coding-standard/bin/symplify-cs fix tests"
        ]
    }
}
