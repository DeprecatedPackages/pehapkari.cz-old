{
    "name": "pehapkari/website",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "symplify/statie": "^4.0",
        "symplify/package-builder": "^4.0"
    },
    "require-dev": {
        "doctrine/annotations": "^1.6",
        "doctrine/cache": "^1.7",
        "doctrine/orm": "^2.6",
        "latte/latte": "^2.4",
        "nette/application": "^2.4",
        "nette/forms": "^2.4",
        "nette/security": "^2.4",
        "symfony/console": "^4.0",
        "symfony/event-dispatcher": "^4.0",
        "symfony/expression-language": "^4.0",
        "symfony/intl": "^4.0",
        "symfony/validator": "^4.0",
        "phpstan/phpstan": "^0.9",
        "phpunit/phpunit": "^7.0",
        "symplify/easy-coding-standard": "dev-master",
        "symplify/coding-standard": "^4.0",
        "spatie/http-status-check": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Pehapkari\\Website\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pehapkari\\Website\\Tests\\": "tests"
        }
    },
    "scripts": {
        "check-cs": "vendor/bin/ecs check src tests",
        "fix-cs": [
            "sh ./bin/clear_post_trailing_whitespaces.sh",
            "vendor/bin/ecs check src tests --fix"
        ],
        "phpstan": "vendor/bin/phpstan analyse src tests --level max --configuration phpstan.neon",
        "check-status": [
            "vendor/bin/http-status-check scan https://pehapkari.cz --output invalid-links.txt"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
